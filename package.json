  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "fp-ts": "^2.0.5",
    "io-ts": "^2.0.1",
    "json-schema-strictly-typed": "^0.0.14"
  },
  "scripts": {
    "compile": "tsc",
    "lint:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
